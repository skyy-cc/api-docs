                        "required": true,
                        "type": "string",
                        "format": "domain"
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "description": "Type of records to check for",
                        "required": true,
                        "type": "string",
                        "format": "type"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "$ref": "#/definitions/ApiResponse"
                        }
                    }
                },
                "security": [
                    {
                        "api_key": [],
                        "api_key_alt": []
                    }
                ]
            }
